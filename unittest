    /**
     * Test of itrmultiplyAB method, of class JavaApplication1.
     */
    @Test
    public void testItrmultiplyAB() {
        System.out.println("itrmultiplyAB");
        int[][] a = {{2,2},{2,2}};
        int[][] b = {{3,3},{3,3}};
        int[][] c = {{12,12},{12,12}};
        
        JavaApplication1.A= a;
        JavaApplication1.B= b;
        JavaApplication1.m= 2;
        JavaApplication1.n= 2;
        JavaApplication1.x= 2;
        JavaApplication1.y= 2;
        int[][] k= JavaApplication1.itrmultiplyAB();
        Assert.assertArrayEquals(c, k);
        // TODO review the generated test code and remove the default call to fail.
       
    }

