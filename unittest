    @Test
    public void testSttmultiplyAB() {
        System.out.println("sttmultiplyAB");
        int[][] a = {{2,2,2,2},{2,2,2,2},{2,2,2,2},{2,2,2,2}};
        int[][] b = {{3,3,3,3},{3,3,3,3},{3,3,3,3},{3,3,3,3}};
